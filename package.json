    "@types/html-webpack-plugin": "^3.2.1",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^13.1.4",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/send": "^0.14.5",
    "@types/webpack": "^4.41.1",
    "@types/webpack-sources": "^0.1.5",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
