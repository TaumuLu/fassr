    "react",
    "ssr"
  ],
  "homepage": "https://github.com/TaumuLu/lissom#readme",
  "dependencies": {
    "ansi-html": "0.0.7",
    "chalk": "^3.0.0",
    "etag": "1.8.1",
    "find-up": "^4.1.0",
    "fresh": "0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "htmlescape": "1.1.1",
    "http-status": "^1.4.2",
    "js-base64": "^2.5.1",
    "qs": "^6.9.1",
